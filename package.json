{
    "name": "calendar",
    "version": "1.0.0",
    "description": "",
    "main": "./src/index.tsx",
    "scripts": {
        "test": "charge-sdk test",
        "start": "ts-node ./src/devServer.ts",
        "build": "charge-sdk build",
        "serve": "ts-node ./src/server.ts",
        "test-dev": "jest --env=jsdom --watch"
    },
    "keywords": [],
    "author": "Istv√°n Antal <i@istvan.xyz>",
    "license": "MIT",
    "jest": {
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ],
        "coverageDirectory": "report",
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "cobertura"
        ],
        "coveragePathIgnorePatterns": [
            "/src/index.ts",
            "/src/server.ts",
            "/src/build.ts",
            "/src/data.ts",
            "/src/actions/index.ts",
            "/src/reducers/index.ts",
            "/src/store/index.ts",
            "/src/store/middleware/",
            "/src/views"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)",
            "<rootDir>/src/**/__tests__/**/*.ts?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
        ],
        "testEnvironment": "node",
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\](?!deepmerge).+\\.(js|jsx|ts|tsx)$"
        ],
        "globals": {
            "ts-jest": {
                "tsConfig": "./tsconfig.json"
            }
        }
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.8",
        "@fortawesome/free-solid-svg-icons": "^5.5.0",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "@types/body-parser": "^1.17.0",
        "@types/dav": "^1.7.0",
        "@types/express": "^4.16.0",
        "@types/jest": "^23.3.9",
        "@types/luxon": "^1.4.1",
        "@types/react": "^16.7.6",
        "@types/react-dom": "^16.0.9",
        "@types/react-redux": "^6.0.9",
        "@types/ws": "^6.0.1",
        "body-parser": "^1.18.3",
        "charge-sdk": "^0.3.12",
        "dav": "^1.8.0",
        "express": "^4.16.4",
        "luxon": "^1.7.1",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-redux": "^5.1.1",
        "redux": "^4.0.1",
        "ws": "^6.1.2"
    },
    "reactTsRuntime": {
        "compilerMiddleware": "./src/build"
    }
}
